{
  "compilerOptions": {
    "types": ["node"],
    "module": "ESNext",
    "target": "ES2020",
    "lib": ["DOM", "ES2020"],
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    //"outFile": "./dist/main.js",
    "outDir": "./dist"
  },
  "include": ["src/**/*.ts"]
}